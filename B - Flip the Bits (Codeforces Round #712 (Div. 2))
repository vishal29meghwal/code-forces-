#include <iostream>
#include <bits/stdc++.h>
#define ll long long
using namespace std;

void it(){
    int n ;
    cin>>n;
    vector<pair<int,int>>x;
    int one = 0 , zero = 0;
    string a,b;
    cin>>a>>b;
    int j = 0 ;
    for(int i = 0 ; i < n ; i++)
    {
        if(a[i]=='1')one++;
        else zero++;
        if(one==zero){
            x.push_back({j,i});
            j = i+1;
        }
    }
    for(auto i : x)
    {
        int start = i.first;
        int end = i.second;
        if(a[start]==b[start])continue;
        else{
            for(int j = start ; j <= end ; j++)
            {
                if(a[j]=='1')a[j]='0';
                else a[j]='1';
            }
        }
    }
    for(int i = 0; i< n ; i++)
    {
        if(a[i]!=b[i])
        {
            cout<<"NO"<<"\n";
            return;
        }
    }
    cout<<"YES\n";
}  
int main(){
        int t=1;
        cin>>t;
        for(int i=1;i<=t;i++){
            it();
        }
    return 0;
}
