#include <bits/stdc++.h>
#include <iostream>
#define ll long long 
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n],b[n];
    for(int i = 0 ; i < n ; i++){
        cin>>a[i];
        b[i]=a[i];
    }
    sort(b,b+n);
    map<int,int> m;
    for(int i = 0 ; i < n ; i++){
        m[b[i]]=i;
    }
    for(int i = 0 ; i < n ; i++){
        a[i]=m[a[i]];
    }
    int l = -1;
    for(int i = 0 ; i< n ;i++){
        if(a[i]!=i){
        l = i+1;
        break;
        }
    }
    int r = -1;
    for(int i = n-1 ; i>=0 ;i--){
        if(a[i]!=i){
        r = i+1;
        break;
        }
    }
    if(l==-1 and r==-1){
        cout<<"yes"<<endl;
        cout<<1<<" "<<1<<endl;
    }else {
        reverse(a+l-1,a+r);
        for(int i = 0 ; i< n ;i++){
        if(a[i]!=i){
            cout<<"no"<<endl;
            break;
        }
        }
        cout<<"yes"<<endl;
        cout<<l<<" "<<r<<endl;
    }
    
    
    return 0;
}
